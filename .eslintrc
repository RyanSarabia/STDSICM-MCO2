{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": [0],
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  }
}
